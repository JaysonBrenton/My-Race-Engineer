{
  "name": "my-race-engineer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0 --port 3001",
    "build": "next build",
    "start": "next start --hostname 0.0.0.0 --port 3001",
    "lint": "eslint --max-warnings 0 --cache --cache-location .cache/eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "fmt": "prettier --write --cache \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "typecheck": "tsc --noEmit",
    "check:types": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "env:doctor": "tsx tools/env-doctor.ts",
    "env:sync": "tsx tools/env-sync.ts",
    "config:doctor": "tsx tools/tsconfig-types-guard.ts",
    "test:seo": "tsx --test tests/seo.test.ts",
    "test:auth": "tsx --test tests/core/auth/*.test.ts",
    "test:auth:e2e": "tsx --test tests/core/auth/auth-flow.e2e.test.ts",
    "test:cookie:unit": "tsx --test tests/server/runtime/cookies.strategy.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:liverc": "LIVERC_E2E=1 tsx --test tests/connectors/liverc/live-smoke.test.ts",
    "test:liverc:discover": "tsx --test tests/connectors/liverc/discover.route.test.ts",
    "test:liverc:service": "tsx --test tests/core/liverc/discovery.service.test.ts",
    "test:liverc:plan": "tsx --test tests/connectors/liverc/plan.route.test.ts",
    "test:liverc:apply": "tsx --test tests/connectors/liverc/apply.route.test.ts",
    "test:ui": "tsx --test --test-reporter=spec tests/app/dashboard/quick-import.form.test.tsx",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --max-warnings=0 --fix"
    ],
    "*.{ts,tsx,js,jsx,css,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.49.1",
    "@opentelemetry/instrumentation": "^0.49.1",
    "@opentelemetry/instrumentation-fetch": "^0.49.1",
    "@opentelemetry/instrumentation-http": "^0.49.1",
    "@opentelemetry/resources": "^1.24.0",
    "@opentelemetry/sdk-trace-base": "^1.24.0",
    "@opentelemetry/sdk-trace-node": "^1.24.0",
    "@opentelemetry/semantic-conventions": "^1.24.0",
    "@prisma/client": "^5.20.0",
    "argon2": "^0.44.0",
    "next": "^15.5.6",
    "node-html-parser": "^7.0.1",
    "nodemailer": "^6.9.13",
    "pino": "^9.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.56.0",
    "@types/node": "^20.14.8",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.5.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^8.0.3",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "tsx": "^4.16.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
